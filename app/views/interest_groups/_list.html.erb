<%-
  param_name ||= :page
  replace ||= 'interest_groups'
-%>

<div id="interest_groups">
  <% paginated_section interest_groups, :class => "act-replace", :rel => replace, :param_name => param_name do %>
    <ul>
    <%- interest_groups.each do |interest_group| -%>
      <li><%= render interest_group %></li>
    <%- end -%>
    </ul>
  <% end %>
</div>
