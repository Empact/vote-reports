<% form_tag url, :method => :get, :'data-replace' => replace do %>
  Show Representatives:
  <%= label_tag :from_where, 'From Where?' %>
  <%= text_field_tag :from_where, params[:from_where] %>
  <%= check_box_tag :in_office, '1', params[:in_office] %>
  <%= label_tag :in_office, 'In Office Only' %>
  <%= submit_tag 'Go!' %>
<% end %>
